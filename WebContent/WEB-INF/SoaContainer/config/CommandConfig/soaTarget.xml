<?xml version="1.0" encoding="gb2312"?>
<commandConfig>

	<targets>

	  <!--远程的支持分布式事务的TARGET-->
		<target name="remoteETAX">
    		<tfield name="type">cn.com.jdls.foundation.architectures.command.targets.ETaxTarget</tfield>
    		<tfield name="contextFactory">weblogic.jndi.WLInitialContextFactory</tfield>
    		<tfield name="url">t3://192.168.2.11:6517</tfield>
    		<tfield name="lookupName">TaxCommand</tfield>
    		<tfield name="remote">true</tfield>
		</target>

	  <!--远程的不支持分布式事务的TARGET-->		
		<target name="remoteETAXNoTx">
				<tfield name="type">cn.com.jdls.foundation.architectures.command.targets.ETaxNoTxTarget</tfield>
				<tfield name="contextFactory">weblogic.jndi.WLInitialContextFactory</tfield>
				<tfield name="url">t3://192.168.70.7:6510</tfield>
				<tfield name="lookupName">TaxCommandNoTx</tfield>
				<tfield name="remote">true</tfield>
		</target>  

		<!--异步消息队列TARGET-->
		<target name="JMSTarget">
    		<tfield name="type">cn.com.jdls.foundation.architectures.command.targets.JMSTarget</tfield>
    		<tfield name="initContextFactory">weblogic.jndi.WLInitialContextFactory</tfield>
    		<tfield name="providerURL">t3://localhost:6517</tfield>
    		<tfield name="factoryJNDI">ETaxFactory</tfield>
    		<tfield name="queueJNDI">FrontQ</tfield>
		</target>
    		
		<!--本地调用TARGET-->    
    <target name="localTarget">
     		<tfield name="type">cn.com.jdls.foundation.architectures.command.targets.LocalTarget</tfield>
    </target>    
			
		<!-- 权限管理EJB连接的配置信息-->
		<target name="remotePerm">
			<tfield name="type">cn.com.jdls.foundation.architectures.command.targets.ETaxTarget</tfield>
			<tfield name="contextFactory">weblogic.jndi.WLInitialContextFactory</tfield>
			<tfield name="url">t3://192.168.2.55:6517</tfield>
			<tfield name="lookupName">PERMCommand</tfield>
			<tfield name="remote">true</tfield>
		</target>
	        
	  <!--SOA WINDOWS构件协议适配器服务-->
		<target name="SoaAdapterForWindowsCOM">
			<tfield name="type">cn.com.jdls.foundation.architectures.command.targets.SoaAdapterForWindowsCOM</tfield>
			<tfield name="serverIp">192.168.70.7</tfield>
			<tfield name="serverPort">12345</tfield>
			<tfield name="loginPassword">lhy</tfield>
		</target> 
		      
  </targets>
  
</commandConfig>
